               },
               {
                  "name":"password",
                  "in":"formData",
                  "description":"Vendor password",
                  "required":true,
                  "type":"string"
               },
               {
                  "name":"phone_no",
                  "in":"formData",
                  "description":"Phone number of vendor",
                  "required":true,
                  "type":"integer"
               },
               {
                  "name":"country_id",
                  "in":"formData",
                  "description":"Country Id",
                  "required":true,
                  "type":"integer"
               },
               {
                  "name":"surcharge",
                  "in":"formData",
                  "description":"Surcharge",
                  "required":false,
                  "type":"string"
               },
               {
                  "name":"status",
                  "in":"formData",
                  "description":"User status active or inactive",
                  "required":true,
                  "type":"integer"
               }                                                                 
            ],
            "responses":{
               "200":{
                  "description":"Vendor created successfully!",
                  "schema":{
                     "$ref":"#/definitions/200add"
                  }
               },
               "400":{
                  "description":"Unable to create vendor!",
                  "schema":{
                     "$ref":"#/definitions/400add"
                  }
               },
               "403":{
                  "description":"You don't have permission to list vendor!",
                  "schema":{
                     "$ref":"#/definitions/403add"
                  }
               },   
               "500":{
                  "description":"Unable to process result!",
                  "schema":{
                     "$ref":"#/definitions/500add"
                  }
               }              
            }
         }
      },
      "/vendor/delete":{
         "delete":{
            "tags":[
               "Vendor"
            ],
            "description":"Delete Vendor Details",
            "operationId":"delete",
            "produces":[
               "application/json"
            ],
            "parameters":[
               {
                  "name":"token",
                  "in":"formData",
                  "description":"Token",
                  "required":true,
                  "type": "string"
               },                    
               {
                  "name":"id",
                  "in":"path",
                  "description":"The Vendor ID.",
                  "required":true,
                  "type":"integer",
                  "minimum": 1
               }
            ],
             "responses":{
               "200":{
                  "description":"Vendor deleted successfully.",
