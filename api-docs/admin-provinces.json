   ],
   "paths":{
      "/admin/province/getprovince/":{
         "post":{
            "tags":[
               "Admin Province"
            ],
            "description":"List all provice from admin section",
            "operationId":"for the URL",
            "produces":[
               "application/json"
            ],
            "parameters":[
               {
                  "name":"token",
                  "in":"formData",
                  "description":"Token get on login api response",
                  "required":true,
                  "type":"string"
               },
               {
                  "name":"province_name",
                  "in":"formData",
                  "description":"Province name search",
                  "required":false,
                  "type":"string"
               },
               {
                  "name":"short_code",
                  "in":"formData",
                  "description":"Province short code search",
                  "required":false,
                  "type":"string"
               }  
            ],
            "responses":{
               "200":{
                  "description":"All provinces listed.",
                  "schema":{
                      "$ref":"#/definitions/200getprovince"
                  }
               },
               "400":{
                  "description":"Page not found",
                  "schema":{
                     "$ref":"#/definitions/400getprovince"
                  }
               }                                
            }
         }
      },
      "/admin/province/createprovince/":{
         "post":{
            "tags":[
               "Admin Province"
            ],
            "description":"Create Provinces",
            "operationId":"for the URL",
            "produces":[
               "application/json"
            ],
            "parameters":[
               {
                  "name":"token",
                  "in":"formData",
                  "description":"Token get on login api response",
                  "required":true,
                  "type":"string"
               },
               {
                  "name":"country_id",
                  "in":"formData",
                  "description":"Country id",
                  "required":true,
                  "type":"integer"
               },
               {
                  "name":"timezone_id",
                  "in":"formData",
                  "description":"Timezone id",
                  "required":true,
                  "type":"integer"
               },
               {
                  "name":"province_name",
                  "in":"formData",
                  "description":"Province name",
                  "required":true,
                  "type":"string"
               },
               {
                  "name":"short_code",
                  "in":"formData",
                  "description":"Short code",
                  "required":true,
                  "type":"string"
               },
               {
                  "name":"location_tax",
                  "in":"formData",
                  "description":"Location tax",
                  "required":true,
                  "type":"integer"
               },
               {
                  "name":"status",
                  "in":"formData",
                  "description":"Status",
                  "required":true,
                  "type":"integer"
               }    
            ],
            "responses":{
               "200":{
                  "description":"New province has been created.",
                  "schema":{
                      "$ref":"#/definitions/200createprovince"
                  }
               },
               "409":{
                  "description":"The specified province already exists.",
                  "schema":{
                     "$ref":"#/definitions/409createprovince"
                  }
               },
               "400":{
                  "description":"Page not found",
                  "schema":{
                     "$ref":"#/definitions/400createprovince"
                  }
               }                                
            }
         }
      },
