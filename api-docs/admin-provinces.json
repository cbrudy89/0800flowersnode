         "get":{
            "tags":[
               "Admin Province"
            ],
            "description":"View single record during update",
            "operationId":"get",
            "produces":[
               "application/json"
            ],
            "parameters":[
               {
                  "name":"id",
                  "in":"path",
                  "description":"The Province ID.",
                  "required":true,
                  "type":"integer",
                  "minimum": 1
               },               
               {
                  "name":"token",
                  "in":"header",
                  "description":"Token get on login api response",
                  "required":true,
                  "type":"string"
               }
            ],
             "responses":{
